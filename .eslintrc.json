{
    "extends": ["next/core-web-vitals", "next/typescript"],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "@typescript-eslint/no-unused-vars": [
            "error",
            { "argsIgnorePattern": "^_" }
        ],
        "@typescript-eslint/no-explicit-any": "warn",
        "@typescript-eslint/triple-slash-reference": "off"
    }
}
